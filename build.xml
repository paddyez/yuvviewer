<project default="compile" basedir=".">
  <target name="compile" depends="clean">
    <javac srcdir="src/main/org/yuvViewer" source="11" includeantruntime="false" optimize="true">
      <compilerarg value="-Xlint:deprecation" />
    </javac>
  </target>
  <target name="clean">
    <delete><fileset dir="src/main/org/yuvViewer" includes="**/*.class"/></delete>
    <delete><fileset dir="src/main/org/yuvViewer" includes="**/*~"/></delete>
  </target>
</project>
